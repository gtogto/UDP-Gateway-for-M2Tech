<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gto.m2techgateway">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="Client"
            ></activity>

        <activity
            android:name=".sub_Activity.CAM_mjpg_player"
            ></activity>

        <activity
            android:name=".sub_Activity.Door_Control"
            ></activity>

        <activity
            android:name=".sub_Activity.Lidar_CANFD"
            ></activity>

        <activity
            android:name=".sub_Activity.i2c_temperature_Activity"
            ></activity>

    </application>

</manifest>